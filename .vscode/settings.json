{
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "filewatcher.commands": [
        {
            "match": "\\.proto",
            "event": "onFileChange",
            "cmd": "${workspaceRoot}\\Proto\\protoc --plugin=protoc-gen-ts_proto=${workspaceRoot}\\node_modules\\.bin\\protoc-gen-ts_proto.cmd --proto_path=${workspaceRoot}\\Proto --ts_proto_opt=onlyTypes=true --ts_proto_out=${workspaceRoot}\\TypeScript\\Game\\Interface2 ${file}"
        }
    ],
    "filewatcher.autoClearConsole": false
}